{
  "name": "tob-ui-core-2",
  "version": "0.0.1",
  "description": "tob-ui 下一个版本的核心库",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "vite build",
    "dev": "vite build --watch",
    "coverage": "vitest --coverage",
    "play": "pnpm -C examples/vue-project run dev",
    "play:open": "pnpm -C examples/vue-project run dev:open",
    "play:host": "pnpm -C examples/vue-project run dev:host",
    "play:build": "pnpm -C examples/vue-project run build",
    "play:preview": "pnpm -C examples/vue-project run preview",
    "play:preview:open": "pnpm -C examples/vue-project run preview:open",
    "play:preview:host": "pnpm -C examples/vue-project run preview:host",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dishait/tob-ui-core-2.git"
  },
  "keywords": [
    "vue3",
    "tob-ui",
    "uniapp",
    "modern",
    "components",
    "composition-api"
  ],
  "author": {
    "name": "markthree",
    "email": "1801982702@qq.com",
    "url": "https://github.com/markthree"
  },
  "homepage": "https://github.com/markthree",
  "bugs": {
    "email": "1801982702@qq.com",
    "url": "https://github.com/dishait/tob-ui-core-2"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.21",
    "bumpp": "^7.1.1",
    "vite": "^2.8.6",
    "vitest": "^0.5.9"
  },
  "dependencies": {
    "vue": "^3.2.31"
  }
}